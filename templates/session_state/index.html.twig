{% extends 'base.html.twig' %}

{% block title %}SessionState index{% endblock %}

{% block body %}
    <h1>SessionState index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>State</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for session_state in session_states %}
            <tr>
                <td>{{ session_state.id }}</td>
                <td>{{ session_state.state }}</td>
                <td>
                    <a href="{{ path('app_session_state_show', {'id': session_state.id}) }}">show</a>
                    <a href="{{ path('app_session_state_edit', {'id': session_state.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_session_state_new') }}">Create new</a>
{% endblock %}
